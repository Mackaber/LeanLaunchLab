body#landing
{
  padding:0;
  margin:0;

  #wrapp {
  }
  .wrap-inner {
    width:962px;
    margin:0 auto;
  } 
  /*Top Area*/
  .topbar{
    background:url(<%= asset_path 'v2/landing-topbg.jpg' %>) 0 0 repeat-x;
    height:51px;
  }
  .topbar div > ul .active > a, .nav .active > a {
    background:none;
    color:#EA7823;
  }
  .topbar div > ul .active > a:hover, .nav .active > a:hover {
    background:none;
    color:#F18330;
  }
  /*Landing Banner*/
  #landing-banner {
    background:url(<%= asset_path 'v2/landing-banner-repeat.jpg' %>) 0 0 repeat-x;
    border-bottom:9px solid #393C3F;
  }
  .landing-banner {
    background:url(<%= asset_path 'v2/landing-bannerbg.jpg' %>) 0 0 no-repeat;
    height:390px;
    position:relative;
  
  }
  .video {
    /*-moz-box-shadow: 0 0 7px #5F5F5F;
    -webkit-box-shadow: 0 0 7px #5F5F5F;
    box-shadow: 0 0 7px #5F5F5F;*/
    background:url(<%= asset_path 'v2/player.png' %>) 0 0 no-repeat;
    position:absolute; bottom:-24px; left:28px;
    width:606px; height:300px;
  }
  .video div {
    padding:4px;
    position:relative;
  }
  .video div p {
    position:absolute; left:225px; top:37px;
    font:bold 25px Arial, Helvetica, sans-serif;
    color:#fff;
    padding:0; margin:0;
    text-align:center;
  }
  .video div p span {
    display:block;
    text-align:center;
    color:#9A9A9A;
    font-size:16px;
    font-weight:normal;
  }
  .video p {
    padding:0; margin:3px 0 0;
    line-height:9px;
  }
  .video small {
    font:normal 10px Arial, Helvetica, sans-serif;
  }
  .video div a.play{
    position:absolute; top:101px; left:233px;
    display:block;
    background:url(<%= asset_path 'v2/play-btn.png' %>) 0 0 no-repeat;
    width:125px; height:125px;
    font-size:0; text-indent:-10000px;
    outline:none;
  }
  .video div a.play:hover{
    background:url(<%= asset_path 'v2/play-btn-hover.png' %>) 0 0 no-repeat;
  }
  .landing-banner h1 {
    font:normal 46px 'Inder', sans-serif;
    color:#fff;
    text-shadow:1px 1px 2px #49494A;
    text-transform:none;
    padding:10px 0 0 40px; margin:0;
    line-height:normal;
  }
  .landing-banner h1.heading {
    background:url(<%= asset_path 'v2/landing-heading-text.png' %>) 0 0 no-repeat;
    height:52px; width:902px;
    font-size:0; text-indent:-100000px;
    padding:0; margin:0;
    line-height:normal;
    position:absolute; left:40px; top:20px;
  }
  .landing-banner h1.heading2 {
    background:url(<%= asset_path 'v2/landing-heading-text2.png' %>) 0 0 no-repeat;
    height:52px; width:902px;
    font-size:0; text-indent:-100000px;
    padding:0; margin:0;
    line-height:normal;
    position:absolute; left:40px; top:20px;
  }
  .landing-banner h1.heading3 {
    background:url(<%= asset_path 'v2/landing-heading-text3.png' %>) 0 0 no-repeat;
    height:52px; width:920px;
    font-size:0; text-indent:-100000px;
    padding:0; margin:0;
    line-height:normal;
    position:absolute; left:40px; top:20px;
  }
  .landing-banner h1 span {
    color:#FA9143;
  }
  a.demo {
    position:absolute; right:30px; top:120px;
    width:270px; height:84px;
    display:block; text-align:center;
    color: #fff;
    font-size: 32px;
    line-height:84px;
    text-decoration:none;
    font-family: 'Inder', sans-serif;
    background-color: #B2B4B7;
    background-repeat: repeat-x;
    background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#C8CCCF), color-stop(50%, #BFC2C5), to(#B2B4B7));
    background-image: -webkit-linear-gradient(#C8CCCF, #BFC2C5 50%, #B2B4B7);
    background-image: -moz-linear-gradient(top, #C8CCCF, #BFC2C5 50%, #B2B4B7);
    background-image: -ms-linear-gradient(#C8CCCF, #BFC2C5 50%, #B2B4B7);
    background-image: -o-linear-gradient(#C8CCCF, #BFC2C5 50%, #B2B4B7);
    background-image: linear-gradient(#C8CCCF, #BFC2C5 50%, #B2B4B7);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#C8CCCF', endColorstr='#B2B4B7', GradientType=0);
    padding:0;
    text-shadow: 0 1px 1px #555758;
    border-bottom:2px solid #535354;
    border:1px solid #7B7B7D;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    -moz-box-shadow: 0 0 1px #5F5F5F;
    -webkit-box-shadow: 0 0 1px #5F5F5F;
    box-shadow: 0 0 1px #5F5F5F;
  }
  a.demo:hover {
    color:#333333;
    text-shadow: 0 1px 1px #fff;
  }
  p.sponsor {
    position:absolute; right:15px; top:90px;
    width:250px; height:79px;
    display:block; text-align:left;
    font-family: 'HelveticaNeueLTPro67MediumCon', 'Inder', sans-serif;
    color: #fff;
    text-shadow: 0 1px 1px #000;
    font-size: 23px; line-height:26px;
    background:url(<%= asset_path 'v2/banner-text-bg.png' %>) 0 0 no-repeat;
    width:286px; height:123px;
    padding:5px 0 0 20px;
    -moz-box-shadow:none;
    -webkit-box-shadow:none;
    box-shadow: none;

  }
  p.sponsor sup {
    font-size:13px;
  }
  a.signup {
    position:absolute; right:30px; top:235px;
    padding:14px 0 0;
    width:270px; height:70px;
    display:block; text-align:center;
    color: #fff;
    font-size: 32px;
    line-height:30px;
    text-decoration:none;
    font-family: 'Inder', sans-serif;
    background-color: #E4843D;
    background-repeat: repeat-x;
    background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#FC9344), color-stop(50%, #F28D41), to(#E4843D));
    background-image: -webkit-linear-gradient(#FC9344, #F28D41 50%, #E4843D);
    background-image: -moz-linear-gradient(top, #FC9344, #F28D41 50%, #E4843D);
    background-image: -ms-linear-gradient(#FC9344, #F28D41 50%, #E4843D);
    background-image: -o-linear-gradient(#FC9344, #F28D41 50%, #E4843D);
    background-image: linear-gradient(#FC9344, #F28D41 50%, #E4843D);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#FC9344', endColorstr='#E4843D', GradientType=0);
    text-shadow: 0 1px 1px #555758;
    border-bottom:2px solid #535354;
    border:1px solid #7B7B7D;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    -moz-box-shadow: 0 0 1px #5F5F5F;
    -webkit-box-shadow: 0 0 1px #5F5F5F;
    box-shadow: 0 0 1px #5F5F5F;
  }
  a.signup:hover {
    color:#4f2506;
    text-shadow: 0 1px 1px #fff;
  }
  a.signup:hover span {
    color:#feecde;
  }
  a.signup span {
    font-size:16px;
    text-shadow:none;
    display:block;
  }
  a.pricing {
    position:absolute; right:50px; bottom:25px;
    color:#EBEBEB;
    font-family: 'Inder', sans-serif;
    font-size: 14px;
    line-height:16px;
    text-decoration:none;
    padding-right:20px;
    background:url(<%= asset_path 'v2/pricing-arrow.gif' %>) 100% 4px no-repeat;
  }
  a.pricing:hover {
    color:#fff;
    text-decoration:underline;
  }
  /*Feature*/
  .land-feature {
    background:url(<%= asset_path 'v2/landing-featurebg.jpg' %>) 0 0 repeat;
    /*border-bottom:9px solid #A1A5A5;*/
  }
  .glow {
    background:url(<%= asset_path 'v2/glow.png' %>) 50% 100% no-repeat;
    padding:30px 0;
    position:relative;
    overflow:hidden;
  }
  ul.left {
    float:left;
  }
  .sponsor {
    float:right;
    width:220px;  
    display:block; text-align:left;
    text-decoration:none;
    font-family: 'Inder', sans-serif;
    background: #c8cccf; /* Old browsers */
    background: -moz-linear-gradient(top, #c8cccf 0%, #bfc2c5 45%, #b2b3b7 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#c8cccf), color-stop(45%,#bfc2c5), color-stop(100%,#b2b3b7)); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top, #c8cccf 0%,#bfc2c5 45%,#b2b3b7 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top, #c8cccf 0%,#bfc2c5 45%,#b2b3b7 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top, #c8cccf 0%,#bfc2c5 45%,#b2b3b7 100%); /* IE10+ */
    background: linear-gradient(top, #c8cccf 0%,#bfc2c5 45%,#b2b3b7 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#c8cccf', endColorstr='#b2b3b7',GradientType=0 ); /* IE6-9 */
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
    -moz-box-shadow: 0 0 5px #7d7d7a;
    -webkit-box-shadow: 0 0 5px #7d7d7a;
    box-shadow: 0 0 5px #7d7d7a;
    padding:10px 25px 20px;
  }
  .sponsor h3 {
    widows:83px;
    height:23px;
    font-size:0;
    text-indent:-100000px;
    background:url(<%= asset_path 'v2/sponsorText.png' %>) 0 0 no-repeat;
    margin-bottom:5px;
  }
  .sponsor a {
    display:block;
    width:221px;
    height:74px;
    background:url(<%= asset_path 'v2/logoes-bg.png' %>) 0 0 no-repeat;
    margin:0 auto 5px;
  }
  .sponsor a img {
    display:block;
    float:left;
    margin:5px 0 0 5px;
  }

  .land-feature ul {
    padding:20px 0 0; margin:0;
    list-style:none;
  }
  .land-feature ul li {
    background:url(<%= asset_path 'v2/landing-feature-icons.png' %>) 0 14px no-repeat;
    font:normal 26px/45px "Century Gothic", Arial;
    color:#6E6E7A;
    padding:0 0 0 60px;
    margin-bottom:12px;
  }
  .land-feature ul li.track {
    background-position:0 -42px;
  }
  .land-feature ul li.store {
    background-position:0 -98px;
  }
  .land-feature ul li.keep {
    background-position:0 100%;
  }
  /*Testimonial*/
  .land-testimonial {
    background:url(<%= asset_path 'v2/landing-testimomnialbg.jpg' %>) 0 0 repeat;
    border-bottom:9px solid #DEDEDE;
  }
  .land-testimonial h3 {
    font:bold 24px "trebuchet MS", Arial, serif;
    color:#464646;
    text-align:center;
    padding:0; margin:15px 0;
  }

  .land-testimonial .article {
    -moz-box-shadow: 5px 5px 4px #9BA19F;
    -webkit-box-shadow: 5px 5px 4px #9BA19F;
    box-shadow: 5px 5px 4px #9BA19F;
    -webkit-border-radius: 8px;
    -moz-border-radius: 8px;
    border-radius: 8px;
    width:289px; /*min-height:220px;*/
    background-color:#fff;
    margin:0 0 35px 0;
    position:relative;
    font:normal 13px/15px "Century Gothic", Arial;
    color:#363636;
  }
  .land-testimonial .article-column {
    width:290px;
    float:left;
    margin-right:30px;
  } 
  .land-testimonial .article img.arrow {
    position:absolute; left:20px; bottom:-20px;
  }
  .land-testimonial .article div {
    padding:15px 10px 10px 15px;
  }
  .land-testimonial .article div p {
    padding:0; margin:0 0 0 57px;
  }
  .land-testimonial .article div img {
    float:left;
    margin-bottom:-100px;
    padding-bottom:30px;
  }
  /*Story **/
  .story-block {
    color: #2D2D2D;
      font-size: 13px;
      line-height: 20px;
      padding:20px 0;
      text-align: justify;
    font-family:"Century Gothic", Helvetica, arial, serif;
    background-color:#fff;
    border-bottom:9px solid #F1F0F0;
  }
  .story-block a {
    color: #2D2D2D;
    text-decoration:underline;
  }
  .story-block a:hover {
    text-decoration:none;
  }
  .story-block h2 {
      font: "Century Gothic", Helvetica, arial, serif;
    font-size:33px;
    font-weight:normal;
    line-height:normal;
      margin: 0 0 10px;
    padding:0;
  }
  .story-block .intro h3 {
      color: #1C1A24;
      font: 26px/32px Arial,Helvetica,sans-serif;
      margin: 3px 0 0;
    text-align:left;
  }
  .story-block .intro {
      margin: 0 0 18px -2px;
      overflow: hidden;
      padding: 0;
  }
  .story-block p {
      margin: 20px 0 0;
  }
  .story-block .intro img {
      background:url(<%= asset_path 'v2/bg-photo-article.png' %>) 0 0 no-repeat;
      float: left;
      height: 103px;
      margin: 0 20px 0 0;
      padding: 7px 7px 21px 8px;
      width: 96px;
  }
  /*Footer*/
  #footer {
    display:block;
    height:58px;
    width:100%;
    position:relative;
    background:url(<%= asset_path 'v2/landing-footerbg.jpg' %>) 0 0 repeat-x;
  }
  #footer p {
    text-align:left;
    margin:0;
    padding-top:0px;
    padding-left:0;
    font-weight:bold;
    color:#7C7E80
  }
  .footer-logo {
    margin:4px 0 0 0px;
  }
  #footer ul {
    padding-top:7px;
    padding-right:0;
  }
  #footer ul.icons {
    padding-top:0;
  }
  #footer ul.icons li {
    background:none;
    padding:0 3px;
  }
  #footer ul li a {
    color:#7C7E80;
    text-shadow:1px 1px 1px #fff;
    text-transform:uppercase;
    font:bold 11px "trebuchet MS", Arial, serif;
  }
  #footer ul li a:hover {
    color:#5B5D5E;
  }
  /*Sponsor*/
  .nav a img.new {
    position:absolute; left:-20px; top:41%;
  }
  .sponsor-body {
    background:url(<%= asset_path 'v2/sponsor-body-bg.jpg' %>) 0 0 repeat;
  }
  .sponsor-top-body {
    background:url(<%= asset_path 'v2/sponsor-top-body-bg.png' %>) 0 0 repeat-x;
  }
  .legal-advice {
    font:normal 17px/25px "Helvetica Neue", Helvetica, Arial, "Lucida Grande", sans-serif;
    color:#000;
    height:100%;
    overflow:hidden;
  }
  .legal-advice .left {
    width:657px;
    float:left;
  }
  .legal-advice .right {
    width:250px;
    padding:0;
    float:right;
  }
  .legal-advice p {
    padding:0; margin:0 0 8px;
  }
  .legal-advice h2 {
    font:normal 47px "Helvetica Neue", Helvetica, Arial, "Lucida Grande", sans-serif;
    color:#000;
    padding:0; margin:8px 0;
  }
  .legal-advice h2 strong {
    color:#82c23a;
  }
  .legal-advice .morgon {
    width:216px;
    border:1px solid #ebebeb;
    -moz-box-shadow: 0px 0px 8px #eaeaea;
    -webkit-box-shadow: 0px 0px 8px #eaeaea;
    box-shadow: 0px 0px 8px #eaeaea;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
    background-color:#fff;
    padding:13px;
    margin-top:25px;
  }
  .legal-advice .morgon h3 {
    font-size:14px;
    color:#000;
    border-bottom:1px solid #efefef;
    padding:0 0 8px; margin:0;
  }
  .legal-advice .round-body {
    border:1px solid #dadada;
    -moz-box-shadow: 0px 0px 8px #dadada;
    -webkit-box-shadow: 0px 0px 8px #dadada;
    box-shadow: 0px 0px 8px #dadada;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
    background-color:#fff;
    padding:1px;
    margin:10px;
    overflow:hidden;
  }
  .legal-advice .round-body h3 {
    font-size:20px;
    line-height:42px;
    color:#000;
    text-shadow:1px 1px 1px #fff;
    border-bottom:1px solid #efefef;
    -webkit-border-top-left-radius: 4px;
    -webkit-border-top-right-radius: 4px;
    -moz-border-radius-topleft: 4px;
    -moz-border-radius-topright: 4px;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    padding:0 0 0 20px; margin:0;
    background:url(<%= asset_path 'v2/round-heading-bg.jpg' %>) 0 0 repeat-x;
  }
  .legal-advice .round-body h3 img {
    margin:7px 9px 0 0;
    display:block;
    float:left;
  }
  .legal-advice .round-body ul {
    padding:0; margin:0; list-style:none;
  }
  .legal-advice .round-body ul li {
    width:133px;
    float:left;
    font-size:13px;
    margin:15px 25px;
    text-align:center;
  }
  .legal-advice .round-body ul li a {
    color:#0066cc;
    font-size:12px;
    text-decoration:underline;
  }
  .legal-advice .round-body ul li a:hover {
    text-decoration:none;
  }
  .legal-advice .round-body ul li img {
    display:block;
    margin:8px 0;
    border:1px solid #acacac;
  }
  .legal-advice .round-body p {
    padding:10px 20px; margin:0;
    font-size:14px;
    line-height:normal;
  }
  .legal-advice .round-body p strong {
    display:block;
    font-size:16px;
  }
  /*Pricing*/
  .price-top-body {
    background:url(<%= asset_path 'v2/sponsor-top-body-bg.png' %>) 0 -140px repeat-x;
  }
  .pricing-wrap {
    width:974px;
    margin:0 auto;
  }
  .pricing {
    position:relative;
  }
  .pricing h2 {
    margin:18px 0 18px 18px;
  }
  .pricing-offer {
    background:url(<%= asset_path 'v2/offer-icon.png' %>) 0 0 no-repeat;
    width:141px;
    height:141px;
    position:absolute; top:0; right:0;
    text-align:center;
    font-size:14px;
    line-height:15px;
    color:#fff;
  }
  .pricing-offer span.price {
    color:#82c23a;
    font-size:31px;
    font-weight:bold;
    line-height:normal;
    display:block;
    padding-top:20px;
  }
  .signup-faq {
    background:url(<%= asset_path 'v2/pricing-signupfaq-bg.png' %>) 0 0 no-repeat;
    padding:20px 42px;
  }
  .signup-faq .signup-area {
    float:left;
    width:533px;
  }
  .signup-faq .signup-area form {
    padding-top:40px;
  }
  .signup-faq .signup-header {
    background:url(<%= asset_path 'v2/pricing-signupbar.png' %>) 0 0 no-repeat;
    width:570px;
    height:46px;
    position:absolute; left:5px;
    font:bold 19px/34px Arial, Helvetica, sans-serif;
    color:#fff;
    text-transform:uppercase;
    text-indent:32px;
  }
  .signup-faq .signup-header span {
    font-size:16px;
    font-style:italic;
    font-weight:normal;
    text-transform:none;
  }
  .signup-faq .signup-section {
    border-bottom:1px dotted #969696;
    padding:19px 0;
    overflow:hidden;
    font:normal 14px Arial, Helvetica, sans-serif;
    color:#3f3f3f;
  }
  .signup-faq .signup-section-last {
    border:none;
  }
  .signup-faq .signup-section small {
    font-size:12px;
  }
  .signup-faq .signup-section small a {
    color:#0664b4;
    text-decoration:none;
  }
  .signup-faq .signup-section small a:hover {
    text-decoration:underline;
  }
  .signup-faq .signup-section h3 {
    padding:0; margin:0 0 8px;
    font:bold 16px Arial, Helvetica, sans-serif;
    color:#000;
    text-transform:uppercase;
  }
  .signup-faq .signup-area input {
    border:1px solid #dcdcdc;
    background-repeat: repeat-x;
    background-image: -khtml-gradient(linear, left top, left bottom, from(#F1F1F1), to(#FFFFFF));
    background-image: -moz-linear-gradient(top, #fff, #F1F1F1);
    background-image: -ms-linear-gradient(top, #fff, #F1F1F1);
    background-image: -webkit-gradient(linear, left top, left bottom, color-stop(100%, #F1F1F1), color-stop(0%, #FFFFFF));
    background-image: -webkit-linear-gradient(top, #F1F1F1, #FFFFFF);
    background-image: -o-linear-gradient(top, #F1F1F1, #FFFFFF);
    background-image: linear-gradient(top, #F1F1F1, #FFFFFF);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#F1F1F1', endColorstr='#FFFFFF', GradientType=0);
    font-size:12px; font-weight:bold; color:#B3B3B3;
    width:280px;
    height:29px;
    -webkit-border-radius: 0;
    -moz-border-radius: 0;
    border-radius: 0;
    margin-bottom:6px;
    color:#000;
    font-weight:bold;
    font-size:15px;
    font-family:Arial, Helvetica, sans-serif;
    padding:0 5px;
  }
  .signup-faq .signup-area input.none {
    width:auto; height:auto;
    background:none;
    border:none;
  }
  .signup-faq .signup-area .signup-bttn {
    background-color:#C5C3C3;
    border:1px solid #8E8E8E;
    line-height:27px;
