    color:#737373;
    padding-left:7px;
  }
  .journal-updates span.invalid {
    background:url(<%= asset_path 'v2/invalid-icon.png' %>) 0 50% no-repeat;
    color:#CA1818;
    padding-left:10px;
    line-height:14px;
  }
  .journal-updates span.hypothesis {
    background:url(<%= asset_path 'v2/completed-small.png' %>) 0 50% no-repeat;
    padding-left:11px;
    text-decoration:none;
  }
  .journal-updates span.tested {
    padding-left:11px;
    color:#C5B124;
    text-decoration:none;
    background:none;
  }
  .journal-updates span.notstarted {
    padding-left:11px;
    color:#353535;
    text-decoration:none;
    background:none;
  }

  .journal-updates p {
    padding:5px 0;
    margin:0;
    font:normal 13px/14px Arial, Helvetica, sans-serif;
    color:#333333;
  }
  .journal-updates:hover {
    background-color:#f8f6f6;
  }
  .hypotheses-validated {
    background-color:#edf8da;
  }
  .hypotheses-validated:hover {
    background-color:#dbedbc;
  }
  .not-started {
    background-color:#F3F3F3;
  }
  .not-started:hover {
    background-color:#E4E4E4;
  }
  .hypotheses-tested {
    background-color:#FFF9CE;
  }
  .hypotheses-tested:hover {
    background-color:#FAF1AF;
  }
  .hypotheses-invalidated {
    background-color:#faeae8;
  }
  .hypotheses-invalidated:hover {
    background-color:#ecceca;
  }

  .completed .askbar {
    background-color:#c0d69a;
  }
  .journal-updates .comment {
    font:normal 11px/18px Verdana, Arial, Helvetica, sans-serif;
    color:#b2b2b2;
    position:relative;
    margin-top:5px;
  }
  .journal-updates .comment a.comm {
    color:#b2b2b2;
    text-decoration:none;
    background:url(<%= asset_path 'v2/comment-bg.gif' %>) 0 50% no-repeat;
    padding-left:12px;
    margin-right:6px;
  }
  .journal-updates .comment a.comm:hover {
    color:#858383;
  }
  .journal-updates .comment a.name {
    color:#336699;
    text-decoration:none;
    margin-right:6px;
  }
  .journal-updates .comment a.name span {
    color:#AAAAA9;
  }
  .journal-updates .comment a.name:hover {
    color:#4F91D2;
  }
  .journal-updates .comment a.attach {
    color:#b2b2b2;
    text-decoration:none;
    background:url(<%= asset_path 'v2/attach-icon.png' %>) 0 50% no-repeat;
    padding-left:10px;
    margin-right:6px;
  }
  .journal-updates .comment a.attach:hover {
    color:#858383;
  }
  .journal-updates .comment a.view {
    color:#b2b2b2;
    text-decoration:none;
    background:url(<%= asset_path 'v2/eye-icon.gif' %>) 0 50% no-repeat;
    padding-left:16px;
  }
  .journal-updates .comment a.view:hover {
    color:#858383;
  }

  .journal-updates h3 {
    font:bold 16px/20px Arial, Helvetica, sans-serif;
    color:#000;
    margin-left:5px;
  }
  .journal-updates h3 a {
    color:#000;
    text-decoration:none;
  }
  .journal-updates h3 a:hover {
    color:#565656;
  }
  .journal-updates .askbar {
    background-color:#F2F2F2;
    padding:4px;
    font:normal 11px/14px Arial, Helvetica, sans-serif;
    color:#999999;
    position:relative;
    margin-top:8px;
    margin-bottom:6px;
  }
  .journal-updates .askbar a {
    color:#4971a7;
    text-decoration:none;
  }
  .journal-updates .askbar a:hover {
    color:#317ce2;
  }
  .journal-updates .askbar span {
    font-weight:bold;
    color:#000;
    font-style:normal;
  }
  .journal-updates .posted {
    font:normal 10px/18px Arial, Helvetica, sans-serif;
    color:#b2b2b2;
    position:relative;
    padding-left:22px;
    margin-left:5px;
    margin-top:20px;
  }
  .journal-updates .posted img {
    position:absolute;
    left:0;
    top:0;
  }
  .journal-updates .posted a {
    color:#b2b2b2;
    text-decoration:none;
  }
  .journal-updates .posted a:hover {
    color:#858383;
  }
  ul.schedule {
    padding:0; margin:0 0 8px;
    list-style:none;
  }
  ul.schedule li {
    float:left;
    padding-right:15px;
  }
  ul.schedule li a {
    font:bold 12px/16px Arial, Helvetica, sans-serif;
    color:#999999;
    text-decoration:none;
  }
  ul.schedule li a:hover,
  ul.schedule li a.selected {
    color:#3F3F3F;
    font-weight:bold;
  }

  .blue-box {
    width:77px;
    height:75px;
    margin-right:18px;
    float:left;
    border:3px solid #012A63;
    background:url(<%= asset_path 'v2/blue-box-bg.jpg' %>) 0 0 repeat-x;
  }
  .blue-box:hover {
    background-position:0 100%;
  }
  .blue-box a,
  .grey-box a {
    text-decoration:none;
    display:block;
  }
  .grey-box {
    width:77px;
    height:75px;
    float:left;
    margin-right:18px;
    margin-bottom:10px;
    border:3px solid #B6B6B6;
    background:url(<%= asset_path 'v2/grey-box-bg.jpg' %>) 0 0 repeat-x;
  }
  .grey-box:hover {
    background-position:0 100%;
  }
  .grey-box span {
    color:#000;
    text-shadow:1px 1px 1px #fff;
  }
  span.date {
    display:block;
    text-align:center;
    font:normal 30px/35px Arial, Helvetica, sans-serif;
    color:#fff;
    text-shadow:1px 1px 1px #012A63;
  }
  span.update {
    display:block;
    text-align:center;
    font:normal 11px/13px Arial, Helvetica, sans-serif;
    color:#fff;
  }
  span.interviews {
    display:block;
    text-align:center;
    font:normal 11px/13px Arial, Helvetica, sans-serif;
    color:#000;
    text-shadow:none;
  }



  p.jbar {
    font:normal 12px/14px Verdana, Arial, Helvetica, sans-serif;
    color:#aaaaa9;
    padding:2px 4px;
    margin-bottom:10px;
    padding-left:0;
    margin:0;
    position:relative;
  }
  p.details {
    color:#AAAAA9;
    padding:3px 0;
  }
  p.details a {
    color:#0066CC;
    font-size:12px;
  }
  p.jbar img {
    position:absolute;
    left:13px;
    top:7px;
  }
