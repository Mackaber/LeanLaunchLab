body#projects
{
  .topbar div > ul .active > a, .nav .active > a { background:none;}

  div#projects{
    width:24%;
    float:left;
    height:90%;
  }
  div.projects-sidebar {
    width:100%;
    height:100%;
    overflow:auto;
    border: 1px solid #ddd;
    border-top:none;
  }
  div#updates{
    width:74.5%;
    height:90%;
    float:left;
    margin-left:13px;
  }
  div.journal-updates-sidebar {
    width:100%;
    height:100%;
    overflow:auto;
  }
  ul.topul {
    background:url(<%= asset_path 'v2/dashbord-twotonebg.jpg' %>) 0 0 repeat-x;
    height:30px;
    padding:0;
    margin:0 0 4px;
    overflow:hidden;
    position:relative;
    text-align:right;
    border:1px solid #666666;
  }
  ul.topul li:last-child {
    margin-right:5px;
  }
  ul.topul li {
    text-align:right;
    padding:3px 0;
    margin:0;
    list-style:none;
    display:inline-block;
  }
  ul.topul li.first {
    padding-left:10px;
    padding-right:10px;
  }
  ul.topul li h2 {
    font:bold 14px/35px Arial,Helvetica,sans-serif;
    color:#fff;
    text-transform:uppercase;
    position:absolute;
    left:8px;
    top:-1px;
  }
  ul.topul li a {
    font:bold 12px/23px Arial, Helvetica, sans-serif;
    color:#FEFEFE;
    display:inline-block;
    position:relative;
    text-shadow:1px 1px 1px #222222;
    padding:1px;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
  }
  ul.topul li a:hover {
    text-decoration:none;
    border-top:1px solid #515151;
    border-left:1px solid #525252;
    border-bottom:1px solid #6E6E6E;
    border-right:1px solid #6E6E6E;
    background-color:#1d1d1d;
    -webkit-box-shadow: inset 0 1px 1px #989999;
    -moz-box-shadow: inset 0 1px 1px #989999;
    box-shadow: inset 0 1px 1px #989999;
    padding:0px;
  }
  ul.topul li img {
    margin-left:6px;
  }
  ul.topul li a.group {
    background:url(<%= asset_path 'v2/group-icon.png' %>) 4px 4px no-repeat;
    padding-left: 30px ;
      padding-right: 4px ;
  }
  ul.topul li a.group:hover {
    background-position:3px -30px;
    padding-left: 29px ;
      padding-right: 3px ;
  }
  ul.topul li a.slide {
    background:url(<%= asset_path 'v2/slide-icon.png' %>) 4px 4px no-repeat;
    padding-left: 25px ;
      padding-right: 4px ;
  }
  ul.topul li a.slide:hover {
    background-position:4px -27px;
  }
  ul.topul li a.announcement {
    background:url(<%= asset_path 'v2/announcement-icon.png' %>) 4px 4px no-repeat;
    padding-left: 25px ;
      padding-right: 4px ;
  }
  ul.topul li a.announcement:hover {
    background-position:4px -27px;
    padding-left: 24px ;
      padding-right: 3px ;
  }

  ul.topul li.update {
    float:right;
  }
  ul.topul li.update a {
    background:url(<%= asset_path 'v2/update-icon.png' %>) 8px 4px no-repeat;
    padding-left:25px;
  }
  ul.topul li.update a:hover {
    background-position:8px -26px;
    background-color:#1d1d1d;
  }
  .playbottom {
    background:url(<%= asset_path 'v2/playbottom.jpg' %>) 0 0 repeat-x;
    -webkit-border-radius: 0 0px 4px 4px;
    -moz-border-radius: 0 0px 4px 4px;
    border-radius: 0 0px 4px 4px;
    height:22px;
    border:none;
  }
  .all-projects {
    background-color:#355991;
    border:2px solid #012A63;
    font:bold 14px/29px Arial, Helvetica, sans-serif;
    color:#fff;
    text-transform:uppercase;
    padding:0 0 0 8px;
    margin:0 0 4px;
  }
  .all-projects span {
    display:inline-block;
    background-color:#e7e7e7;
    -webkit-border-radius: 1px;
    -moz-border-radius: 1px;
    border-radius: 1px;
    font:bold 11px/14px Arial, Helvetica, sans-serif;
    color:#747474;
    text-align:center;
    padding:0 4px;
    text-transform:none;
  }
  .all-projects span:hover {
    background-color:#e7e7e7;
  }

  ul.group {
    padding:0;
    margin:0;
  }
  ul.group li {
    list-style:none;
    padding:0;
    margin:0 0 1px;
    position:relative;
  }
  ul.group li a {
    font:bold 14px/25px Arial, Helvetica, sans-serif;
    color:#000;
    text-transform:uppercase;
    text-decoration:none;
  }
  ul.group li a:hover {
    color:#393838;
  }
  ul.group li div.projest-label {
    background-color:#fff;
    border:1px solid #C0C0C0;
    padding-left:6px;
  }
  ul.group li div.projest-label img {
    margin:0 6px;
  }

  ul.group li div.projest-label:hover {
    background-color:#e7e7e7;
  }
  ul.group li div.projest-label a {
    display:inline-block;
  }
  ul.group li div.projest-label a span {
    color:#0066CC;
    font-size:11px;
    text-transform:none;
    font-weight:bold;
  }

  ul.group li ul {
    padding:0;
    margin:0;
  }
  ul.group li li {
    margin-top:1px;
    background-color:#fff;
    border:1px solid #C0C0C0;
  }
  ul.group li li:hover {
    background-color:#F7F7F7;
  }
  ul.group li li a {
    font-size:11px;
    text-transform:none;
  }
  ul.group li li a span {
    display:inline-block;
    font:normal 11px/14px Arial, Helvetica, sans-serif;
    color:#747474;
    text-align:center;
    padding:0 4px;
    background-color:#E2E2E2;
    text-transform:none;
  }
  ul.group li li a span:hover {
    background-color:#D1D1D1;
  }
  ul.group li li a.verticle-dot {
    background:url(<%= asset_path 'v2/group-project-bg.gif' %>) 0 0 no-repeat;
    width:9px;
    height:13px;
    display:inline-block;
    margin:0 0 0 3px;
  }
  ul.group li li a.verticle-dot:hover {
    background-position:0 100%;
  }


  ul.group li li a.green-dot {
    background:url(<%= asset_path 'v2/green-circle.gif' %>) 0 0 no-repeat;
    width:10px;
    height:10px;
    display:inline-block;
    margin:0 6px;
  }
  ul.group li li a.green-dot:hover {
    background-position:0 100%;
  }
  ul.group li li a.pro-can-on {
    background:url(<%= asset_path 'v2/project-canvas-icon.gif' %>) 0 0 no-repeat;
    width:25px;
    height:15px;
    display:inline-block;
    margin:0 6px 0 0;
    position:relative;
    top:3px;
  }
  ul.group li li a.pro-can-off {
    background:url(<%= asset_path 'v2/project-canvas-icon.gif' %>) 0 100% no-repeat;
    width:25px;
    height:15px;
    display:inline-block;
    margin:0 6px 0 0;
    position:relative;
    top:3px;
  }
  ul.group li li a.yellow-dot {
    background:url(<%= asset_path 'v2/yellow-circle.gif' %>) 0 0 no-repeat;
    width:10px;
    height:10px;
    display:inline-block;
    margin:0 6px;
  }
  ul.group li li a.yellow-dot:hover {
    background-position:0 100%;
  }
  ul.group li li a.red-dot {
    background:url(<%= asset_path 'v2/red-circle.gif' %>) 0 0 no-repeat;
    width:10px;
    height:10px;
    display:inline-block;
    margin:0 6px;
  }
  ul.group li li a.red-dot:hover {
    background-position:0 100%;
  }



  /*Right Area*/
  .journal-updates {
    background-color:#fff;
    border:1px solid #DCDBDB;
    padding:8px 12px;
    margin-bottom:4px;
    overflow:hidden;
    position:relative;
  }
