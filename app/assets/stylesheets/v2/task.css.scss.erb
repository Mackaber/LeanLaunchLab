body#task {
  padding:0;
  margin:0;
  
  input, textarea, button {
    -webkit-border-radius: 0;
    -moz-border-radius: 0;
    border-radius: 0;
  }
  div#wrapper h1 {
    margin:14px 0 0 13px;
  }
  div#wrapper h1 span {
    display:block;
    font-size:14px;
    line-height:14px;
    color:#010101;
  }
  ul.toplink {
    padding:0;
    margin:-12px 18px 0 0;
    text-align:left;
    float:right;
  }
  ul.toplink li {
    text-align:left;
    display:inline-block;
    padding:0 1px 0 0;
    line-height:13px;
    margin:0;
  }
  ul.toplink li:last-child {
    border:none;
  }
  ul.toplink li a {
    color:#333333;
    font-size:12px;
    text-decoration:none;
    border:1px solid #e5e5e5;
    border-bottom-color:#ebebeb;
    display:block;
    padding:0 10px;
    line-height:28px;
      background:url(<%= asset_path 'v2/sub-nav.jpg' %>) 0 0 repeat-x;
  }
  ul.toplink li a:hover {
    color:#626060;
    text-decoration:none;
    background:#ebebeb;
    border:1px solid #dbd9d9;
    border-bottom-color:#ebebeb;
  }
  ul.toplink li a.selected {
    color:#626060;
    text-decoration:none;
    background:#ebebeb;
    border:1px solid #dbd9d9;
    border-bottom-color:#ebebeb;
  }

  /*Columns style*/
  #columns.column-1 .colunn { width: 100%; }
  #columns.column-2 .column { width: 49.5%; float:left; margin-right:1%; }
  #columns.column-2 .column.last { margin-right:0;}

  #columns.column-3 .column { width: 32.5%; float:left; margin-right:.8%; }
  #columns.column-2 .column.last { margin-right:0;}

  /*div#left{
    width:49.5%;
    float:left;
  }*/
  div.left-sidebar {
    width:98%;
    overflow:auto;
    /*top:166px;bottom:10px;left:2.6%;position:absolute;*/
    -webkit-border-radius:0;
    -moz-border-radius: 0;
    border-radius:0;
    background-color:#fff;
    border-top:none;
    padding:1%;
    background-color:#fff;
  }
  /*div#right{
    width:49.5%;
    float:right;
  }*/
  div.right-sidebar {
    width:98%;
    padding:1%;
    overflow:auto;
    /*top:165px;bottom:10px;right:0px; position:absolute;*/
    background:#fff;
  }
  div.right-sidebar h4 {
    font:bold 16px Arial, Helvetica, sans-serif;
    border-bottom:1px solid #CCCCCC;
    padding:0 0 5px;
    margin-bottom:15px;
  }
  div.right-sidebar input.title {
    font:bold 16px Arial, Helvetica, sans-serif;
    color:#333333;
  }
  div.right-sidebar h4 a {
    color:#333333;
    text-decoration:none;
  }
  div.right-sidebar h4 a:hover {
    text-decoration:underline;
  }
  div.right-sidebar ul.list {
    padding:0; margin:0;
    list-style:none;
  }
  div.right-sidebar ul.list li {
    float:left;
    width:15%;
    font:normal 12px/20px Arial, Helvetica, sans-serif;
    color:#999999;
    padding:2px 0;
  }
  div.right-sidebar ul.list li a {
    background:url(<%= asset_path 'v2/task-drop.gif' %>) 100% 50% no-repeat;
    color:#333333;
    padding:0 15px 0 0;
    text-decoration:none;
  }
  div.right-sidebar ul.list li a.no-bg {
    background:none;
  }
  div.right-sidebar ul.list li a:hover {
    text-decoration:underline;
  }
  div.right-sidebar ul.list li.rt {
    width:83%;
    position:relative;
  }
  div.right-sidebar ul.list li.rt input.small {
    width:87px;
    color:#333333;
  }
  div.right-sidebar ul.list li.rt textarea {
    width:98.4%;
    height:120px;
    color:#000;
  }
  div.right-sidebar ul.list li.rt img.cal {
    position:absolute;
    top:6px;
    margin-left:7px;
  }
  div.right-sidebar ul.list li.rt div.dropdown {
    position:absolute;
    top:25px;
    left:0;
    width:270px;
    padding:4px;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    -moz-box-shadow: 0 0 5px #DDDDDD;
    -webkit-box-shadow: 0 0 5px #DDDDDD;
    box-shadow: 0 0 5px #DDDDDD;
    border:1px solid #DDDDDD;
    background:#fff;
    z-index:99999;
  }
  div.right-sidebar ul.list li.rt div.dropdown-small {
    width:150px;
  }
  div.right-sidebar ul.list li.rt div.dropdown a {
    font:normal 11px/25px Arial, Helvetica, sans-serif;
    color:#333333;
    padding:0 7px;
    display:block;
    border-bottom:1px dotted #CCCCCC;
    background:none;
    text-decoration:none;
  }
  div.right-sidebar ul.list li.rt div.dropdown a:hover {
    background:#6699CC;
    color:#fff;
  }
  div.right-sidebar ul.list li.rt div.dropdown a.active {
    background:#6699CC;
    color:#fff;
  }
  div.right-sidebar ul.list li.rt div.dropdown a.last {
    border:none;
  }

  .playbottom {
    background:url(<%= asset_path 'v2/playbottom.jpg' %>) 0 0 repeat-x;
    -webkit-border-radius: 0 0px 4px 4px;
    -moz-border-radius: 0 0px 4px 4px;
    border-radius: 0 0px 4px 4px;
    height:22px;
    border:none;
  }
  ul.topul {
    background-color:#373737;
    padding:0;
    margin:0;
    height:33px;
    overflow:hidden;
    position:relative;
    text-align:right;
    border:1px solid #666666;
    background:url(<%= asset_path 'v2/twotone-header.jpg' %>) 0 0 repeat-x;
  }
  ul.topul li:last-child {
    margin-right:5px;
  }
  ul.topul li {
    text-align:right;
    padding:3px 5px;
    margin:0;
    list-style:none;
    display:inline-block;
  }
  ul.topul li.first {
    padding-left:10px;
    padding-right:10px;
  }
  ul.topul li h2 {
    font:bold 14px/35px Arial, Helvetica, sans-serif;
    color:#fff;
    text-transform:uppercase;
    position:absolute;
    left:8px;
    top:-1px;
  }
  ul.topul li a {
    font:bold 12px/23px Arial, Helvetica, sans-serif;
    color:#FEFEFE;
    display:inline-block;
    position:relative;
    text-shadow:1px 1px 1px #222222;
    padding:1px;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
  }
  ul.topul li a:hover {
    text-decoration:none;
    border-top:1px solid #515151;
    border-left:1px solid #525252;
    border-bottom:1px solid #6E6E6E;
    border-right:1px solid #6E6E6E;
    background-color:#1d1d1d;
    -webkit-box-shadow: inset 0 1px 1px #989999;
    -moz-box-shadow: inset 0 1px 1px #989999;
    box-shadow: inset 0 1px 1px #989999;
    padding:0px;
  }
  /*To do*/
  .step-heading {
    background-color:#FEFBBE;
    border:1px solid #EEE98C;
    border-left:5px solid #EEE98C;
    padding:5px 10px 0px;
    margin-bottom:7px;
  }
  .steps {
    overflow:hidden;
    margin:0 0 8px;
    background:none;
    border:none;
  }
  .steps p {
    padding:0; margin:0 0 4px;
  }

  .steps h3 {
    font:normal 14px Arial, Helvetica, sans-serif;
    color:#3F3F3F;
    margin-bottom:4px;
  }
  .steps h3 a {
    color:#3B6EC3;
    text-transform:uppercase;
    text-transform:uppercase;
    float:right;
    font-size:9px;
    padding-top:3px;
  }
  form { padding:0; margin:0; width:100%;}
  input {
    border:1px solid #DCDCDC;
    background-repeat: repeat-x;
    background-image: -khtml-gradient(linear, left top, left bottom, from(#F1F1F1), to(#FFFFFF));
    background-image: -moz-linear-gradient(top, #fff, #F1F1F1);
    background-image: -ms-linear-gradient(top, #fff, #F1F1F1);
    background-image: -webkit-gradient(linear, left top, left bottom, color-stop(100%, #F1F1F1), color-stop(0%, #FFFFFF));
    background-image: -webkit-linear-gradient(top, #F1F1F1, #FFFFFF);
    background-image: -o-linear-gradient(top, #F1F1F1, #FFFFFF);
    background-image: linear-gradient(top, #F1F1F1, #FFFFFF);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#F1F1F1', endColorstr='#FFFFFF', GradientType=0);
    font-size:12px; font-weight:bold; color:#B3B3B3;
    width:98.7%;
    -webkit-border-radius: 0;
    -moz-border-radius: 0;
    border-radius: 0;
    margin-bottom:4px;
  }
  input.chk {
    width:auto;
    height:auto;
    background:none;
    margin:2px 4px 0 0;
    float:left;
  }
  select {
    border:1px solid #DCDCDC;
    background-repeat: repeat-x;
    background-image: -khtml-gradient(linear, left top, left bottom, from(#F1F1F1), to(#FFFFFF));
    background-image: -moz-linear-gradient(top, #fff, #F1F1F1);
    background-image: -ms-linear-gradient(top, #fff, #F1F1F1);
    background-image: -webkit-gradient(linear, left top, left bottom, color-stop(100%, #F1F1F1), color-stop(0%, #FFFFFF));
    background-image: -webkit-linear-gradient(top, #F1F1F1, #FFFFFF);
    background-image: -o-linear-gradient(top, #F1F1F1, #FFFFFF);
    background-image: linear-gradient(top, #F1F1F1, #FFFFFF);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#F1F1F1', endColorstr='#FFFFFF', GradientType=0);
    font-size:12px; font-weight:bold; color:#B3B3B3;
    width:auto;
    -webkit-border-radius: 0;
    -moz-border-radius: 0;
    border-radius: 0;
    margin-bottom:4px;
  }
  input.number {
    width:95%;
  }
  textarea {
    border:1px solid #DCDCDC;
    background-repeat: repeat-x;
    background-image: -khtml-gradient(linear, left top, left bottom, from(#F1F1F1), to(#FFFFFF));
    background-image: -moz-linear-gradient(top, #fff, #F1F1F1);
    background-image: -ms-linear-gradient(top, #fff, #F1F1F1);
    background-image: -webkit-gradient(linear, left top, left bottom, color-stop(100%, #F1F1F1), color-stop(0%, #FFFFFF));
    background-image: -webkit-linear-gradient(top, #F1F1F1, #FFFFFF);
    background-image: -o-linear-gradient(top, #F1F1F1, #FFFFFF);
    background-image: linear-gradient(top, #F1F1F1, #FFFFFF);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#F1F1F1', endColorstr='#FFFFFF', GradientType=0);
    font-size:14px; font-weight:bold; color:#B3B3B3;
    width:97.5%;
    margin-bottom:4px;
  }
  textarea.details {
    color:#000;
    font-size:12px; line-height:17px; font-weight:normal;
  }
  input.quest {
    color:#B3B3B3;
  }
  p.right-text {
    text-align:right;
    color:#999999;
    font-size:11px;
    line-height:normal;
    float:right;
    padding:0; margin:0;
  }
  p.enter-text {
    text-align:left;
    color:#333333;
    font-size:11px;
    line-height:11px;
    float:left;
  }
  p.right-text a {
    color:#999999;
    text-decoration:none;
  }
  p.right-text a:hover {
    text-decoration:underline;
  }
  p.right-text a.example {
    color:#3B6EC3;
  }
  .tags {
    font-size:12px;
    color:#3F3F3F;
    overflow:hidden;
    padding-bottom:10px;
    background:none;
    border:none;
    margin-left:0;
  }
  .tags a {
    color:#3F3F3F;
    text-decoration:underline;
  }
  .tags a:hover {
    text-decoration:none;
  }
  .tags button  {
    float:none;
    margin-right:4px;
    margin-top:5px;
  }
  button.browse  {
    float:none;
    margin-right:5px;
  }
  ul.hov { 
    padding:0; 
    margin:0 0 4px; 
    list-style:none;
  }
  ul.hov li { 
    padding:2px; 
    margin:0; 
    position:relative; 
    display:block; 
    font-size:12px; 
    color:#000; 
    font-weight:bold;
  }
  ul.hov li a.new-win {
    background:url(<%= asset_path 'v2/new-win-icon.gif' %>) 100% 45% no-repeat;
    padding-right:13px;
  } 
  ul.hov li strong.strikethru {
    text-decoration:line-through; 
  }
  ul.hov li strong.name {
    font-style:italic;
    color:#999999;
  }
  ul.hov li p.details {
    padding:0 30px 0 0;
    margin:0;
  }
  ul.hov li p{ 
    padding:0; 
    margin:0;
  }
  ul.hov li span { 
    display:block; 
    position:absolute; 
    top:3px; right:4px; 
    visibility:hidden; 
    margin-right:15px;
  }
  ul.hov li span.delete { 
    margin-right:0px;
  }
  ul.hov li:hover { 
    border:1px solid #E2DFA4; 
    padding:1px; 
    background-color:#EBE9B5;
  }
  ul.hov li:hover span { 
    visibility:visible;
  }
  p.attachment { 
    font-size:11px;
    color:#999999; 
    padding:0; margin:0;
  }
  p.attachment span {
    color:#999999; 
  }
  p.attachment a {
    color:#426DB5;
    text-decoration:none;
  }
  p.attachment a:hover {
    text-decoration:underline;
  }

  /*Label Start*/
  .start {
    border:1px solid #B5B5B5;
    background-color:#D6D6D6;
    text-shadow:1px 1px 1px #fff;
    color:#5C5C5C;
    right:0;
    padding:0 10px;
  }
  .start:hover {
    -webkit-box-shadow: inset 0 2px 1px #D9D9D9;
    -moz-box-shadow: inset 0 1px 2px #D9D9D9;
    box-shadow: inset 0 2px 1px #D9D9D9;
    background-color:#B5B5B5;
    color:#fff;
    text-shadow:1px 1px 1px #7E7E7E;
    border:1px solid #B5B5B5;
  }

  p span.label {
    background:url(<%= asset_path 'v2/validate-icon.png' %>) 0 50% no-repeat;
    font:normal 10px/16px Arial, Helvetica, sans-serif;
    color:#5a8626;
    text-decoration:none;
    padding-left:10px;
    text-shadow:1px 1px 1px #fff;
    float:right;
    text-transform:uppercase;
  }
  p span.date {
    font:normal 10px/16px Arial, Helvetica, sans-serif;
    background:none;
    color:#898989;
    text-decoration:none;
    padding-right:5px;
    text-shadow:1px 1px 1px #fff;
    float:right;
    text-transform:uppercase;
  }
  p span.unknown {
    background:url(<%= asset_path 'v2/unknown-icon.png' %>) 0 50% no-repeat;
    color:#737373;
    padding-left:7px;
  }

  p span.invalid {
    background:url(<%= asset_path 'v2/invalid-icon.png' %>) 0 50% no-repeat;
    color:#CA1818;
    padding-left:10px;
    line-height:14px;
  }
  p span.hypothesis {
    background:url(<%= asset_path 'v2/completed-small.png' %>) 0 50% no-repeat;
    padding-left:11px;
    text-decoration:none;
  }
  h4 img {
    position:absolute;
    left:0;
    top:0;
  }
  img.arrow {
    position:absolute;
    left:-18px;
    top:50%;
  }
  /*Task*/
  .task{
    padding:0;
    margin:0 0 2px;
    list-style:none;
  }
  .task div {
    position:relative;
    margin-top:5px;
    margin-left:18px; 
    padding:8px 8px 5px 8px;
  }
  .task div.left {
    float:left;
    width:72.5%;
    margin:0;
    padding:0;
  }
  .task div.right {
    float:right;
    width:70px;
    height:100%;
    margin:0;
    padding:0;
  }
  .task div button.validate {
    width:70px;
    margin-bottom:4px;
  }
  .task div button.invalidate {
    width:70px;
    margin-bottom:4px;
  }
  .task div button.unknown {
    width:70px;
  }
  .task div span.label {  
    position:absolute;
    right:4px;
    bottom:4px;
  }
  .task div span.unknown {
    background:url(<%= asset_path 'v2/unknown-icon.png' %>) 0 50% no-repeat;
    color:#737373;
    padding-left:8px;
  }
  .task div span.invalid {
    background:url(<%= asset_path 'v2/invalid-icon.png' %>) 0 50% no-repeat;
    color:#CA1818;
    padding-left:10px;
    line-height:14px;
  }
  .task div span.hypothesis {
    background:url(<%= asset_path 'v2/completed-small.png' %>) 0 50% no-repeat;
    padding-left:11px;
    text-decoration:none;
  }
  .task div span.validated {
    background:url(<%= asset_path 'v2/validate-icon.png' %>) 0 50% no-repeat;
    font:normal 10px/16px Arial, Helvetica, sans-serif;
    color:#5a8626;
    text-decoration:none;
    padding-left:10px;
    text-shadow:1px 1px 1px #fff;
    text-transform:uppercase;
  }
  .task div.task-expanding {
    position:relative;
    margin-top:5px;
    margin-left:0px;  
    padding:6px 8px 12px 8px;
    background-color:#fefbbe;
    border:1px solid #eee98c;
    border-left:5px solid #eee98c;
  }
  .task div.task-expanding .steps,
  .task div.task-expanding .tags {
    margin-bottom:0;
    padding:0 0 0 16px;
  }
  .task div.task-expanding .task-title {
    margin-bottom:4px;
  }
  .task div.task-expanding .task-title a {
    background-position:0 100%;
  }
  .task-heading {
    margin-left:0;
    background-color:#efefef;
    border:1px solid #CFCFCF;
    border-left:5px solid #CFCFCF;
  }
  .task-expanded {
    margin-left:0;
    background-color:#EEE98C;
    padding:5px 1px 1px 4px;
  }
  .task-expanded .step-heading {
    margin-left:0;
    border:none;
    background-color:#FEFBBE;
    padding:0 10px;
  }
  .task-expanded .steps {
    border-bottom:1px dotted #999999;
    padding:5px 0;
    margin:0;
  }
  .task-expanded .steps p strong,
  .task-expanded .steps span  {
    color:#999999;
  }
  .task-expanded .steps p span {
    color:#426DB5;
  }
  .task-expanded .comment-section {
    height:100%;
    overflow:hidden;
    font-size:12px;
    color:#333333;
    padding:0px; margin:0 0 6px;
  }
  .task-expanded button.browse {
    float:none;
    margin-top:5px;
  }
  .task-expanded button.post {
    float:none;
    margin-top:5px;
  }
  .task-expanded .comment-section .profile-pic {
    float:left;
    width:27px;
    padding:0px; margin:0;
  }
  .task-expanded .comment-section .profile-pic img {
    display:block;
    border:1px solid #cfcfcf;
  }

  .task-expanded .comment-section .comment-text {
    padding:0px; margin:0 0 0 37px;
  }
  .task-expanded .comment-section .comment-text div {
    float:left;
    width:100%;
    padding:0px; margin:0;
  }
  .task-expanded .comment-section .comment-text div p.comment-desc {
    color:#999999;
    padding:0;
    margin:0;
    line-height:12px;
  }
  .task-expanded .comment-section .comment-text div textarea.comment {
    padding:4px;
    font:normal 12px Arial, Helvetica, sans-serif;
    color:#999999;
    width:96.5%;
  }
  .task-expanded input.permalink {
    padding:3px;
    font:normal 12px Arial, Helvetica, sans-serif;
    color:#999999;
    width:97.5%;
  }
  .experiment-complete {
    background-color:#efefef;
    border:1px solid #CFCFCF;
    border-left:5px solid #CFCFCF;
  }
  .hypothesis-invalid {
    background-color:#faeae8;
    margin-left:0;
    border:1px solid #FF9999;
    border-left:4px solid #FF9999;
  }
  .hypothesis-heading {
    background-color:#edf8da;
    margin-left:0;
    border:1px solid #C1DB93;
    border-left:4px solid #C1DB93;
  }
  .hypothesis-pending {
    background-color:#ffffda;
    border:1px solid #E2E289;
    border-left:4px solid #E2E289;
  }
  p.task-title {
    font-size:14px;
    font-weight:bold;
    color:#3F3F3F;
    padding:0; margin:0;
    position:relative;
  }
  p.task-title input {
    width:79%;
    margin:0 0 0 4px;
  }
  p.task-title a {
    color:#3F3F3F;
    text-decoration:none;
    background:url(<%= asset_path 'v2/plan-task-tringle.gif' %>) 0 1px no-repeat;
    padding-left:16px;
    display:block;
  }
  p.task-title a:hover {
    text-decoration:none;
  }
  p.task-title a.expanded {
    background-position:0 100%;
    display:inline-block;
  }
  p.task-title a img {
    margin-right:4px;
  }
  p.task-title a span {
    color:#999999;
    font-size:12px;
    font-weight:normal;
  }
  span.updates a {
    background:url(<%= asset_path 'v2/updates-icon.png' %>) 0 50% no-repeat;
    font:normal 11px/15px Verdana, Arial, Helvetica, sans-serif;
    color:#999999;
    padding:0 0 0 10px;
    margin-bottom:2px;
    text-decoration:none;
  }
  span.name a {
    font:normal 11px/15px Verdana, Arial, Helvetica, sans-serif;
    color:#999999;
    padding:0 0 0 16px;
    margin-bottom:2px;
    margin-right:7px;
    text-decoration:none;
  }
  #project .plan span.name a:hover {
    color:#6a6969;
    text-decoration:none;
  }


  span.attach a {
